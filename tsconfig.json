{
  "include": ["src", "tests", ".dumi/**/*", ".dumirc.ts", "*.ts", "docs"],
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "target": "ES2015",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "types": ["vitest/globals"],
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@/*": ["src"],
      "react-selectable-box": ["src"],
      "react-selectable-box/*": ["src/*", "*"]
    }
  }
}
